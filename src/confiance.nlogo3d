extensions [profiler]
;; | =============================
;; | ========== GLOBALS ==========
;; | =============================
globals [
  radius               ;; Radius of the spherical world
  current-round        ;; Current round value (number of iterating round) - Used as a time value
]


;; | =============================
;; | ========== GLOBALS ==========
;; | =============================
breed [providers provider]
breed [consumers consumer]

;; | =====================================
;; | ========== CLASS VARIABLES ==========
;; | =====================================
providers-own [
    utility-gain-mu         ;;
    utility-gain-sigma      ;;
    utility-gain-min        ;;
    utility-gain-max        ;;
    utility-gain            ;; for logging

    profile                 ;; Good or Intermittent or Bad - Used to determine performance level
    radius-of-operation     ;;
    ratings                 ;; List of ratings per agent's id in decreasing order(0 = AgentA, 1 = AgentB, 2 = Interaction, 3 = Value)
    certificates            ;; Ratings to be shown as certificates

    acquaintances           ;; Neighbours in range of operations

    need-update?            ;; Does it need to be need-update? ? (Just spawned or moved)
]

consumers-own                   [
    utility-gain                ;;
    ug                          ;; cache - last utility gained

    activity-level              ;; Probability to request a service, set randomly at spawn
    radius-of-operation         ;;
    ratings                     ;; List of ratings per agent's id in decreasing order(0 = AgentA, 1 = AgentB, 2 = Interaction, 3 = Value)
    certificates                ;; Ratings to be shown as certificates
    trust-model                 ;; Which trust-model is used (NoTrust or FIRE)
    temperature                 ;; Exploitation-Exploration dilemma

    interactions_counter        ;; Number of interaction
    failed_interactions_counter ;; Number of failed interaction (No providers found)

    acquaintances               ;; Neighbours in range of operations
    providers-set               ;; Neighbours in range of operations

    need-update?                ;; Does it need to be need-update? ? (Just spawned or moved)

    use-it?                     ;; Explicit
    use-cr?                     ;; Explicit
    use-wr?                     ;; Explicit
]

;; | ================================
;; | ========== SETUP & GO ==========
;; | ================================

to setup
  clear-all
  set-globals
  spawn-providers number-of-good-providers "good" 105
  spawn-providers number-of-ordinary-providers "ordinary" 85
  spawn-providers number-of-intermittent-providers "intermittent" 125
  spawn-providers number-of-bad-providers "bad" 135

  if (NoTrust-Group?)[
    spawn-consumers number-of-consumers "NoTrust" 65 false false false
  ]

  if (IT-Group?)[
    spawn-consumers number-of-consumers "FIRE" 15 true false false
  ]

  if (IT-CR-Group?)[
    spawn-consumers number-of-consumers "FIRE" 15 true true false
  ]

  if (IT-WR-Group?)[
    spawn-consumers number-of-consumers "FIRE" 15 true false true
  ]

  if (FIRE-Group?)[
    spawn-consumers number-of-consumers "FIRE" 15 true true true
  ]

  reset-ticks
end

to go
    if (current-round >= number-of-rounds)
        [stop]

    set current-round (current-round + 1)
    interaction-round
    tick
end

;; | ===================================
;; | ========== SETUP RELATED ==========
;; | ===================================

to set-globals
    ;;set radius (world-width - 1) / 2
    set radius 1
    set current-round 0
end

to spawn-providers [number _profile _color]
    create-providers number [
        set-xyz-inside-sphere ;; Distribute turtles uniformely inside a sphere
        set color (shaded-color _color)
        set shape providers-shape
        set profile _profile
        set ratings []
        set certificates []
        set acquaintances no-turtles
        set need-update? true
        set radius-of-operation (radius * ((random-float-range min-radius-of-operation max-radius-of-operation) / 100))
        set size 0.025
        set utility-gain 0
        if (_profile = "good")[
            set utility-gain-mu random-float-range 5 10
            set utility-gain-sigma 1.0
            set utility-gain-min 5
            set utility-gain-max 10
        ]
        if (_profile = "ordinary")[
            set utility-gain-mu random-float-range 0 5
            set utility-gain-sigma 1.0
            set utility-gain-min 0
            set utility-gain-max 5
        ]
        if (_profile = "bad")[
            set utility-gain-mu random-float-range -10 0
            set utility-gain-sigma 2.0
            set utility-gain-min -10
            set utility-gain-max 0
        ]
        if (_profile = "intermittent")[
            set utility-gain-mu 0 ;;not used
            set utility-gain-sigma 0 ;;not used
            set utility-gain-min -5
            set utility-gain-max 5
        ]
  ]
end

to spawn-consumers [number _trust-model _color _use-it? _use-cr? _use-wr?]
    create-consumers number [
        set-xyz-inside-sphere ;; Distribute turtles uniformely inside a sphere
        set color (shaded-color _color)
        set activity-level (random-float-range min-activity-level max-activity-level)
        set radius-of-operation (radius * ((random-float-range min-radius-of-operation max-radius-of-operation) / 100))
        set shape consumers-shape
        set size 0.025
        set ratings []
        set certificates []
        set acquaintances no-turtles
        set providers-set no-turtles
        set need-update? true
        set trust-model _trust-model
        set utility-gain 0
        set ug 0
        set interactions_counter 0
        set temperature initial-temperature
        set use-it? _use-it?
        set use-cr? _use-cr?
        set use-wr? _use-wr?
  ]
end


;; | ================================
;; | ========== GO RELATED ==========
;; | ================================

to interaction-round

    ;; ===== PROFILING START =====
    profiler:start
    ;; ==========================

    ask providers[
        if(need-update?)[
            update-provider
        ]
    ]

    ask consumers[
        if(need-update?)[
            update-consumer
        ]

        if(need-service?)[

            ;; providers-set is computed only when needed (When the consumer need an update)

            ifelse(count providers-set > 0)[

                ;; Choose one according to the selection process
                let choosen-provider ([who] of select-provider)

                ;; Interact
                use-service choosen-provider

                ;; Rate
                let rating (new-rating (who) (choosen-provider) (current-round) (ug))
                add-rating (choosen-provider) (rating)

                ;; Inform provider of his rating
                sent-rating choosen-provider rating
            ][
                set failed_interactions_counter (failed_interactions_counter + 1)
            ]
        ]

        ;; ===== Tick Update =====
        set temperature ifelse-value(temperature = 1) [temperature] [temperature - 1]
    ]

    ;; ===== PROFILING STOP =====
    profiler:stop
    print profiler:report
    profiler:reset
    ;; ==========================

end


;; | ===============================
;; | ========== FUNCTIONS ==========
;; | ===============================

;; | ==============================
;; | ========== FORMULAS ==========
;; | ==============================

to-report compute-trust-value [agent]
    let trust-values []
    let wK []
    let coeffs []

    if(use-it?) [
        let value trust-value-it agent
        if(not empty? value)[
            set trust-values (fput (item 0 value) trust-values)
            set wK (fput (interaction-trust-coeff * (item 1 value)) wK)
            set coeffs (fput interaction-trust-coeff coeffs)
        ]
    ]

    if(use-cr?) [
        let value trust-value-cr agent
        if(not empty? value)[
            set trust-values (fput (item 0 value) trust-values)
            set wK (fput (certified-reputation-coeff * (item 1 value)) wK)
            set coeffs (fput certified-reputation-coeff coeffs)
        ]
    ]

    if(use-wr?) [
        let value trust-value-wr agent
        if(not empty? value)[
            set trust-values (fput (item 0 value) trust-values)
            set wK (fput (witness-reputation-coeff * (item 1 value)) wK)
            set coeffs (fput witness-reputation-coeff coeffs)
        ]
    ]

    if(not empty? trust-values and not empty? wK)[
        let trust-value ( (sum (map * wK trust-values)) / (sum coeffs) )
        let reliability ( (sum wK) / (sum coeffs) )

        report (list trust-value reliability)
    ]

    report []
end

;; ========== IT ==========

;; Return the following list (value, reliability) if computable
to-report trust-value-it [agent]
    let _ratings (find-agent-ratings agent)

    ifelse(length _ratings > 0)[
        let numerateur 0
        let denominateur 0
        let _trust-value 0
        let deviation-reliability 0

        ifelse(length _ratings > 1)[
            set numerateur reduce [ [current-value rating] ->
                    current-value + ((pr-it (current-round - (item 2 rating)) (item 3 rating)) * (item 3 rating))
                ] _ratings

            set denominateur reduce [ [current-value rating] ->
                    current-value + (pr-it (current-round - (item 2 rating)) (item 3 rating))
                ] _ratings

            set _trust-value (numerateur / denominateur)

            set deviation-reliability reduce [ [current-value rating] ->
                    current-value + (pr-it (current-round - (item 2 rating)) (item 3 rating)) * abs((item 3 rating) - _trust-value)
                ] _ratings
        ][
            let rating item 0 _ratings

            set numerateur ((pr-it (current-round - (item 2 rating)) (item 3 rating)) * (item 3 rating))
            set denominateur (pr-it (current-round - (item 2 rating)) (item 3 rating))

            set _trust-value (numerateur / denominateur)

            set deviation-reliability (pr-it (current-round - (item 2 rating)) (item 3 rating)) * abs((item 3 rating) - _trust-value)
        ]

        report (list (_trust-value) ((1 - exp ( - interaction-trust-reliability * denominateur)) * (1 - 0.5 * (deviation-reliability / denominateur))))
    ][
        report []
    ]
end


;; IT Rating reliability
to-report pr-it [delta rating-value]
    report exp ( (- delta * rating-value) / recency-scaling-factor )
end


;; ========== CR ==========
;; Same as IT except for the ratings collected
to-report trust-value-cr [agent]
    let _ratings [certificates] of provider agent

    ifelse(length _ratings > 0)[
        let numerateur 0
        let denominateur 0
        let _trust-value 0
        let deviation-reliability 0

        ifelse(length _ratings > 1)[
            set numerateur reduce [ [current-value rating] ->
                    current-value + ((pr-it (current-round - (item 2 rating)) (item 3 rating)) * (item 3 rating))
                ] _ratings

            set denominateur reduce [ [current-value rating] ->
                    current-value + (pr-it (current-round - (item 2 rating)) (item 3 rating))
                ] _ratings

            set _trust-value (numerateur / denominateur)

            set deviation-reliability reduce [ [current-value rating] ->
                    current-value + (pr-it (current-round - (item 2 rating)) (item 3 rating)) * abs((item 3 rating) - _trust-value)
                ] _ratings
        ][
            let rating item 0 _ratings

            set numerateur ((pr-it (current-round - (item 2 rating)) (item 3 rating)) * (item 3 rating))
            set denominateur (pr-it (current-round - (item 2 rating)) (item 3 rating))

            set _trust-value (numerateur / denominateur)

            set deviation-reliability (pr-it (current-round - (item 2 rating)) (item 3 rating)) * abs((item 3 rating) - _trust-value)
        ]

        report (list (_trust-value) ((1 - exp ( - interaction-trust-reliability * denominateur)) * (1 - 0.5 * (deviation-reliability / denominateur))))
    ][
        report []
    ]
    report []
end


;; ========== WR ==========
to-report trust-value-wr [agent]
    report []
end

;; WR Rating reliability
to-report pr-wr [agentA agentB]
    report 0
end


;; WR Deviation reliability
to-report pd-wr [agentA agentB]
    report 0
end


;; ========== Other ==========

;; Returns service's quality value after linear degradation : f(distance) = -(service-quality-degradation-alpha) * distance + world-width
to-report service-quality-degradation [_consumer _provider value]
    let _distance ([distance _provider] of _consumer)
    let degradation (- service-quality-degradation-alpha * (_distance - [radius-of-operation] of _provider) + 1)
    report ifelse-value(  _distance > [radius-of-operation] of _provider) [value * degradation] [value]
end


;; | ===================================
;; | ========== CLASS-RELATED ==========
;; | ===================================

;; Add the given rating to turtle's rating list
to add-rating [agent rating]
    let agent-ratings find-agent-ratings agent
    let index position (list agent agent-ratings) ratings

    ifelse(length agent-ratings = 0)[
        set ratings fput (list agent (list rating)) ratings
    ][
        set agent-ratings (sublist (fput rating agent-ratings) 0 (min (list history-size (length agent-ratings))))
        set ratings replace-item index ratings (list agent agent-ratings)
    ]

    ;; Ajout aux certificats
    set certificates (sublist (fput rating certificates) 0 (min (list history-size (length certificates))))
end

;; Returns specified agent's ratings
to-report find-agent-ratings [agent]
    let results (filter [ elt -> (item 0 elt) = agent] ratings)
    ifelse(length results > 0)[
        report item 1 item 0 results
    ][
        report []
    ]
end


;; Returns a list of providers in radius of operation
;; If none is relevant, ask acquaintance to find one (recursive)
to-report find-providers [excluded_agents length_threshold]
    if(count acquaintances > 0) [
        ;; First - Is there any providers nearby ?
        let _providers (acquaintances with [ breed = providers ])

        ;; Even if we already have providers, let's get others nearby
        if (length_threshold > 0)[

            let _acquaintances (acquaintances with [ (not member? self excluded_agents) and (breed = consumers) ])

            set excluded_agents (turtle-set self excluded_agents _acquaintances)

            ask _acquaintances[
                set _providers (turtle-set
                    _providers
                    find-providers (excluded_agents) (length_threshold - 1))
            ]
        ]
        report _providers
    ]
    report no-turtles
end


;; Returns the selected provider based on the trust model. See 137-138p.
to-report FIRE-providers-selection

    let hasTrustValue_value []
    let hasTrustValue_agent []

    let noTrustValue_agent []

    ;; ===== STEP I =====
    ;; Compute trustworthiness of each providers
    ;; if computable
    ;;    add to HasTrustValue
    ;; else
    ;;    add to NoTrustValue
    foreach (sort providers-set) [ _provider ->
        let value (compute-trust-value [who] of _provider)
        ifelse (empty? value)[
            set noTrustValue_agent (fput _provider noTrustValue_agent)
        ][
            set hasTrustValue_value fput (item 0 value) hasTrustValue_value
            set hasTrustValue_agent (fput _provider hasTrustValue_agent)
        ]
    ]

    ;; ===== STEP II =====
    ;; If one is empty, choose the other
    ;; else, exploitation-exploration dilemma
    ifelse(empty? noTrustValue_agent or empty? hasTrustValue_agent)[
        ifelse(empty? noTrustValue_agent)[
            report item (position (max hasTrustValue_value) hasTrustValue_value) hasTrustValue_agent
        ][
            report one-of noTrustValue_agent
        ]
    ][

        let expected_exploration_return ([utility-gain] of (item (position (max hasTrustValue_value) hasTrustValue_value) hasTrustValue_agent))
        let expected_exploitation_return (mean [utility-gain] of providers-set)

        let normalization-term ( exp ((expected_exploitation_return) / temperature) + exp ((expected_exploration_return) / temperature))

        let pExploration exp ((expected_exploration_return) / temperature ) / normalization-term
        let pExploitation exp ((expected_exploitation_return) / temperature) / normalization-term ;; Can not be used

        ifelse (coin-flip(pExploration))[
            ;; Exploration
            report one-of noTrustValue_agent
        ][
            ;; Exploitation
            report item (position (max hasTrustValue_value) hasTrustValue_value) hasTrustValue_agent
        ]
    ]
end


;; Should the consumer ask for a service ?
to-report need-service?
  report coin-flip activity-level
end


;; Returns a rating based on parameters
to-report new-rating [agentA agentB interaction value]
    report (list agentA agentB interaction value)
end


;; Returns a list of rating matching given parameters
;; If "any" is given as a parameter, the latter won't be used as a filter
to-report query [agentA agentB interaction value]
  let useAgentA (agentA != "any")
  let useAgentB (agentB != "any")
  let useInteraction (interaction != "any")
  let useValue (value != "any")

  let response (
    filter [ elt ->
      (ifelse-value(useAgentA) [(item 0 elt) = agentA] [true]) and
      (ifelse-value(useAgentB) [(item 1 elt) = agentB] [true]) and
      (ifelse-value(useInteraction) [(item 2 elt) = interaction] [true]) and
      (ifelse-value(useValue) [(item 3 elt) = value] [true])
    ] ratings )
  report response
end


;; rate a provider and return the rating
to-report rate [other-agent interaction]
    let value 0
    let rating (new-rating who other-agent interaction value)
    add-rating other-agent rating
    report rating
end


;; Returns the selected provider by the process. The selection process relies on the agent's trust model to decide
;; which provider is likely to be the most reliable. Consumers agents without a trust model randomly
;; select a provider from the list.
to-report select-provider
    if(trust-model = "FIRE") [
        report FIRE-providers-selection
    ]

    report one-of providers-set
end


;; Inform the agent of his rating
to sent-rating [agent rating]
    let consumer_number who
    ask provider agent [
        add-rating consumer_number rating
    ]
end

to update
    set acquaintances (other turtles in-radius radius-of-operation)
    set need-update? false
end

to update-consumer
    update
    set providers-set (find-providers (turtle-set self) (referral-length-threshold))
end

to update-provider
    update
end

;; Returns an utility gain based on provider's service quality
to use-service [choosen_provider]
    set ug [random-utility] of provider choosen_provider
    set interactions_counter interactions_counter + 1
    set utility-gain utility-gain + ug
end


;; | ===================================
;; | ========== WORLD-RELATED ==========
;; | ===================================


;; Set a turtle position to a random point in a sphere
;; From : https://karthikkaranth.me/blog/generating-random-points-in-a-sphere/
;; Uniform Distribution to prevent aggregation in center
to set-xyz-inside-sphere
  let u (random-float 1000)
  let v (random-float 1)

  let theta (2 * pi * u)
  let phi (acos(2 * v - 1))

  let r (random-float 1) ^ (1 / 3)

  let x (r * sin(phi) * cos(theta)) * radius
  let y (r * sin(phi) * sin(theta)) * radius
  let z (r * cos(phi)) * radius

  setxyz x y z
end

;; | =============================
;; | ========== HELPERS ==========
;; | =============================

to-report increment [value]
  report (value + 1)
end

to-report shaded-color [value]
  report (value - 1 + random-float 3)
end

to-report random-float-range [_min _max]
    report (random-float (_max - _min) + _min)
end

to-report random-range [_min _max]
    report (random (_max - _min) + _min)
end

to-report coin-flip [probability]
    report (random-float 1) <= probability
end

to-report normalized [_min _max x]
    report (x - _min) / (_max - _min)
end

to-report random-utility
    ifelse (profile = "intermittent")[
        set utility-gain random-float-range utility-gain-min utility-gain-max
        report utility-gain
    ][
        set utility-gain max( list utility-gain-min min (list utility-gain-max (random-normal utility-gain-mu utility-gain-sigma)))
        report utility-gain
    ]
end

